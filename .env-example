# OAuth Google
CLIENT_ID_GOOGLE=your_client_id_here.apps.googleusercontent.com
SECRET_GOOGLE=GOCSPX-your_secret_here

# Database
# SQLite (desenvolvimento)
DB_URL=sqlite:///./eleitorai.db

# PostgreSQL (produção)
# DB_URL=postgresql://user:password@localhost:5432/eleitorai

# Redis
REDIS_HOST=localhost
